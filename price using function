#include<stdio.h>
void calculateprice(float value);
int main(){
    float value=100.0;
    printf("value is : %f\n",value);//hum apne parameter me koi bhi change kare vo argument me reflect nhi karega
    calculateprice(value);
    return 0;
}
void calculateprice(float value){
    value=value+(0.18*value);
    printf("final value is :%f\n",value);
}
